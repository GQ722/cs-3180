# First we get our list of available characters.

swag consonants = ['B', 'C', 'D', 'F', 'G', 'H', 'J', 'K', 'L', 'M', 'N', 'P', 'Q', 'R', 'S', 'T', 'V', 'W', 'X', 'Y', 'Z', ]
swag vowels = ['A', 'E', 'I', 'O', 'U', ]
swag num_of_vowels = random(6, 12)
# random number of vowels
swag random_vowel = random(vowels)
swag board = [random_vowel, ]
# loop through appending the vowels to the list
naenae i in num_of_vowels
swag random_vowel = random(vowels)
swag new_item = [random_vowel, ]
board = board + new_item
groovy
swag num_of_consonants = 25 - num_of_vowels
# fill the rest of the list with consonants
naenae i in num_of_consonants
swag random_consonant = random(consonants)
swag new_item = [random_consonant, ]
board = board + new_item
groovy
